---
import PlayerLayout from "../components/PlayerLayout.tsx"
import AstroLayout from "../components/layouts/AstroLayout.astro"
import "../styles/global.css"
---

<AstroLayout>
    <div class="w-full min-h-screen px-5 pt-8 pb-12 bg-[#0B0B0B]">
        <PlayerLayout client:load />
    </div>
    <script>
        if ("serviceWorker" in navigator) {
            navigator.serviceWorker.register("/sw.js")
        }
    </script>
</AstroLayout>
